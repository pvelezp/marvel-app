{
  "extends": [
    "plugin:jest/all",
    "eslint:recommended",
    "next",
    "next/core-web-vitals",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "@next/next/no-img-element": "off",
    "jest/no-hooks": "off",
    "prettier/prettier": "error",
    "jest/prefer-expect-assertions": 0
  }
}
